{
  "name": "@nestjs-ssr/react",
  "version": "0.1.0",
  "description": "Elegant React SSR for NestJS. Zero-config, fully typed, production-ready.",
  "keywords": [
    "nestjs",
    "react",
    "ssr",
    "server-side-rendering",
    "vite",
    "typescript",
    "streaming-ssr",
    "hmr"
  ],
  "author": "Georgi Alexandrov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/nestjs-ssr.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/yourusername/nestjs-ssr/issues"
  },
  "homepage": "https://github.com/yourusername/nestjs-ssr#readme",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./render": {
      "types": "./dist/render/index.d.ts",
      "import": "./dist/render/index.mjs",
      "require": "./dist/render/index.js"
    },
    "./monitoring": {
      "types": "./dist/monitoring/index.d.ts",
      "import": "./dist/monitoring/index.mjs",
      "require": "./dist/monitoring/index.js"
    },
    "./vite": {
      "types": "./dist/vite/index.d.ts",
      "import": "./dist/vite/index.mjs",
      "require": "./dist/vite/index.js"
    },
    "./react/*": {
      "types": "./dist/react/*.d.ts",
      "import": "./dist/react/*.mjs",
      "require": "./dist/react/*.js"
    },
    "./templates/*": "./src/templates/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src/templates",
    "src/react",
    "src/global.d.ts",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "prepublishOnly": "pnpm build && pnpm typecheck"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.0 || ^6.0.0"
  },
  "dependencies": {
    "serialize-javascript": "^6.0.2"
  },
  "devDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "@types/express": "^5.0.5",
    "@types/glob": "^9.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/serialize-javascript": "^5.0.4",
    "glob": "^13.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rxjs": "^7.8.2",
    "tsup": "^8.0.0",
    "typescript": "^5.7.2",
    "vite": "^7.0.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
